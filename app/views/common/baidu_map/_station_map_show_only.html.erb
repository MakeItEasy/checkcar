      <script type="text/javascript">
        $(document).ready(function(){
          // 创建Map实例
          var map = new BMap.Map("allmap");
          // 创建点坐标
          var point = new BMap.Point(<%= station.jingdu%>, <%= station.weidu%>);
          map.addOverlay(new BMap.Marker(point));
          // 初始化地图,设置中心点坐标和地图级别。
          map.centerAndZoom(point,15);
        });
      </script>
