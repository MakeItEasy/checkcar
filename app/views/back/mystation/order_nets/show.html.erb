<div class='box car-toolbox'>
  <div class="btn-toolbar" role="toolbar">
    <div class="btn-group pull-right">
      <%= link_to back_mystation_orders_path, {class: 'btn btn-default'} do %>
        <i class="fa fa-list"></i> <%= t('view.action.back_to_list') %> 
      <% end %>
    </div>
  </div>
</div>

<div class='box box-info car-content-box'>
  <dl class="dl-horizontal">
    <dt><%= OrderNet.human_attribute_name(:order_date) %></dt>
    <dd><%= @order_net.order_date%></dd>
    <dt><%= OrderNet.human_attribute_name(:order_time) %></dt>
    <dd><%= @order_net.order_time_text %></dd>
    <dt><%= OrderNet.human_attribute_name(:owner_name) %></dt>
    <dd><%= @order_net.owner_name %></dd>
    <dt><%= OrderNet.human_attribute_name(:car_number) %></dt>
    <dd><%= @order_net.car_number %></dd>
    <dt><%= OrderNet.human_attribute_name(:telephone) %></dt>
    <dd><%= @order_net.telephone %></dd>
  </dl>
</div>
